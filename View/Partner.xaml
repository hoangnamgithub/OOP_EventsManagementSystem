                <Grid>
                    <styles:SearchBar
                        Margin="10"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top" />
                    <ScrollViewer Margin="0,50,0,0" VerticalAlignment="Top">
                        <ItemsControl HorizontalAlignment="Center">
                            <!--  Định nghĩa ItemsPanel là WrapPanel  -->
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <WrapPanel HorizontalAlignment="Center" Orientation="Horizontal" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <!--  Template cho từng mục  -->
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel
                                        Margin="10"
                                        HorizontalAlignment="Center"
                                        Orientation="Vertical">
                                        <Image
                                            Width="150"
                                            Height="150"
                                            Margin="0,0,0,10"
                                            Source="pack://application:,,,/Resources/Images/adele.jpg"
                                            Stretch="UniformToFill">
                                            <Image.Clip>
                                                <EllipseGeometry
                                                    Center="75,75"
                                                    RadiusX="75"
                                                    RadiusY="75" />
                                            </Image.Clip>
                                        </Image>
                                        <TextBlock
                                            Width="150"
                                            HorizontalAlignment="Center"
                                            Foreground="Black"
                                            Style="{StaticResource txt}"
                                            Text="{Binding}"
                                            TextAlignment="Center"
                                            TextWrapping="Wrap" />
                                    </StackPanel>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <!--  Dữ liệu mẫu  -->
                            <ItemsControl.ItemsSource>
                                <x:Array xmlns:sys="clr-namespace:System;assembly=mscorlib" Type="sys:String">
                                    <sys:String>Sauer, Oberbrunner and Rowe</sys:String>
                                    <sys:String>Another Item</sys:String>
