                     HorizontalAlignment="Right">
                <Grid>
                    <styles:SearchBar HorizontalAlignment="Left"
                                      VerticalAlignment="Top"
                                      Margin="10" />
                    <ScrollViewer VerticalAlignment="Top"
                                  Margin="0,50,0,0">
                        <ItemsControl HorizontalAlignment="Center">
                            <!-- Định nghĩa ItemsPanel là WrapPanel -->
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <WrapPanel Orientation="Horizontal"
                                               HorizontalAlignment="Center" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <!-- Template cho từng mục -->
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                Margin="10">
                                        <Image Source="pack://application:,,,/Resources/Images/adele.jpg"
                                               Width="150"
                                               Height="150"
                                               Stretch="UniformToFill"
                                               Margin="0,0,0,10">
                                            <Image.Clip>
                                                <EllipseGeometry Center="75,75"
                                                                 RadiusX="75"
                                                                 RadiusY="75" />
                                            </Image.Clip>
                                        </Image>
                                        <TextBlock Text="{Binding}"
                                                   Style="{StaticResource txt}"
                                                   TextWrapping="Wrap"
                                                   Foreground="Black"
                                                   TextAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   Width="150" />
                                    </StackPanel>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <!-- Dữ liệu mẫu -->
                            <ItemsControl.ItemsSource>
                                <x:Array Type="sys:String"
                                         xmlns:sys="clr-namespace:System;assembly=mscorlib">
                                    <sys:String>Sauer, Oberbrunner and Rowe</sys:String>
                                    <sys:String>Another Item</sys:String>
                                    <sys:String>Additional Item</sys:String>
                                    <sys:String>Extra Item</sys:String>
